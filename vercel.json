{
    "version": 2,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "styles.css",
            "use": "@vercel/static"
        },
        {
            "src": "script.js",
            "use": "@vercel/static"
        },
        {
            "src": "LOGOS/**",
            "use": "@vercel/static"
        },
        {
            "src": "Certificates/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/contact",
            "dest": "/api/contact.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}